<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.VistaController">
   <children>
      <VBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <ImageView fx:id="imgview" pickOnBounds="true" preserveRatio="true" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button onAction="#handleButtonAction" text="Cargar imagen" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
